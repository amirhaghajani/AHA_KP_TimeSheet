<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> سیستم مدیریت برگه زمانی سپاروک</title>

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/Images/KPE/KPE.png">
    <!-- CSS
        ============================================ -->
    <!-- Bootstrap CSS -->
    <link href="~/Content/bootstrap-3.4.1-dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-3.4.1-dist/css/bootstrap.min.rtl.css" rel="stylesheet" />
    <link href="~/Content/Layout/coreui-icons.min.css" rel="stylesheet" />
    <link href="~/Content/Layout/flag-icon.min.css" rel="stylesheet" />
    <link href="~/Content/Layout/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/Layout/simple-line-icons.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/assets/css/vendor/material-design-iconic-font.min.css">
    <link rel="stylesheet" href="~/assets/css/vendor/themify-icons.css">
    <link rel="stylesheet" href="~/assets/css/vendor/cryptocurrency-icons.css">
    <link href="~/Content/RasSideBar.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />


</head>





<body dir="rtl">
    <div id="Loader" class="glass" style="display:none;">
        <div style="position:absolute; right:45%; top:40% "><img style="width:200px; z-index:11000" class="img-responsive" src="~/Images/Cube.gif" /></div>
    </div>

    <div class="main-wrapper">
        <div class="header-section navbar navbar-fixed-top">
            <div class="row">
                <div class="col-xs-5 ">
                    <div class="row">
                        <div class="col-xs-5" style="margin-top:15px">
                            <a href="@Url.Action("Index","Home")" title="خانه">
                                <img src="~/Images/KPE/KPE.png" style="height:50px;display:inline-block;margin-right:30px"
                                     class="image img-responsive" />
                            </a>
                            <a onclick="openNav()" title="منوی اصلی" class="ras-pointer" style="color:white; background-color:transparent;margin-right :20px;margin-top:15px; font-size:30px;display:inline-block;"><i class="ti-view-list"></i></a>
                        </div>
                        <div class="col-xs-6">
                        </div>
                    </div>
                </div>
                <div class="col-xs-4"></div>
                <div class="col-xs-2">
                    <div onclick="ShowAndHideUserMenu()" style="top:20px" class="ras-dropdown ras-border-menu ras-pointer ">
                        <div style="color:white;position:absolute;left:-190px" class="form-inline">
                            <img class="image img-circle img-responsive " style="max-height:30px;left:-50px ;position:absolute" src="~/Images/User.png" />
                            <div style="margin-top:6px">  <b style="font-size:15px">@User.Identity.Name</b></div>
                        </div>

                        <div class="ras-dropdown-content" style="margin-left:5px;">
                            <div class="panel panel-info">
                                <div class="panel-heading">
                                    <h5 style="margin-right:38%">  @User.Identity.Name </h5>
                                    <h4 style="margin-right:25px"> به سیستم خوش آمدید</h4>
                                </div>
                                <div class="panel-body">
                                    <ul style="margin-top:20px">
                                        <li>
                                            <a href="@Url.Action("LoginOtherUser","Home")" style="font-size:15px">
                                                <i style="font-size:50px" class="zmdi zmdi-accounts-alt">
                                                </i>    ورود با کاربر دیگر
                                            </a>
                                        </li>
                                        <li>
                                            <hr />
                                        </li>
                                        <li>
                                            <span>
                                                <i style="font-size:40px " class="zmdi zmdi-lock-open text-danger"></i>
                                                <a style="font-size:15px" class="text-danger" href="@Url.Action("Logout","Home")">
                                                    خروج
                                                </a>
                                            </span>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div id="mySidebar" class="rasrasSidebar">
            <div class="side-header">
                <div class="side-header-inner custom-scroll"  style="margin-top:-50px">
                    <nav class="side-header-menu" id="side-header-menu">
                         
                        <div id="accordion">
                            <a style="color:white" class="ras-sidebar-ul" href="@Url.Action("Index","Home")">
                                <i title="خانه" class="ti-home pull-left ras-sidebar-left-logo" style="font-size:24px ; margin-left:-15px;color:white"></i>
                                <i class="ti-home"></i> <span>خانه</span>

                            </a>
                            <div class="menu-item">
                                <a style="color:white" class="ras-sidebar-ul" data-toggle="collapse" data-parent="#accordion" href="#collapse1">
                                    <i title="برگه زمانی" class="ti-alarm-clock pull-left ras-sidebar-left-logo" style="font-size:24px ; margin-left:-15px;color:white"></i>
                                    <i class="ti-alarm-clock"></i>
                                    برگه زمانی
                                </a>
                                <div id="collapse1" class="panel-collapse collapse">
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("Register","TimeSheets")">
                                        <span>ثبت برگه زمانی</span>
                                    </a>

                                </div>
                            </div>

                            <div class="menu-item">
                                <a style="color:white" class="ras-sidebar-ul" data-toggle="collapse" data-parent="#accordion" href="#collapse2">
                                    <i title="برگه زمانی" class="ti-check-box pull-left ras-sidebar-left-logo" style="font-size:24px ; margin-left:-15px;color:white"></i>
                                    <i class="ti-check-box"></i>
                                    تاییدات برگه زمانی
                                </a>
                                <div id="collapse2" class="panel-collapse collapse">
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("Confirmation","TimeSheets")">
                                        <span>تایید برگه زمانی</span>
                                    </a>

                                </div>
                            </div>
                            <div class="menu-item">
                                <a style="color:white" class="ras-sidebar-ul" data-toggle="collapse" data-parent="#accordion" href="#collapse3">
                                    <i title="ثبت مرخصی و ماموریت" class="ti-car pull-left ras-sidebar-left-logo" style="font-size:24px ; margin-left:-15px;color:white"></i>
                                    <i class="ti-car"></i>
                                    ثبت مرخصی و ماموریت
                                </a>
                                <div id="collapse3" class="panel-collapse collapse">
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("Index","DailyLeaves")">
                                        <span>ثبت مرخصی روزانه</span>
                                    </a>
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("Index","HourlyLeaves")">
                                        <span>ثبت مرخصی ساعتی</span>
                                    </a>
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("Index","HourlyMissions")">
                                        <span>ثبت ماموریت ساعتی</span>
                                    </a>
                                </div>
                            </div>

                            <div class="menu-item">
                                <a style="color:white" class="ras-sidebar-ul" data-toggle="collapse" data-parent="#accordion" href="#collapse4">
                                    <i title=" تاییدات مرخصی و ماموریت" class="zmdi zmdi-check-all pull-left ras-sidebar-left-logo" style="font-size:24px ; margin-left:-15px;color:white"></i>
                                    <i class="zmdi zmdi-check-all"></i>
                                    تاییدات مرخصی و ماموریت
                                </a>
                                <div id="collapse4" class="panel-collapse collapse">
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("ApproveIndex","DailyLeaves")">
                                        <span>تایید مرخصی روزانه</span>
                                    </a>
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("ApproveIndex","HourlyLeaves")">
                                        <span>تایید مرخصی ساعتی</span>
                                    </a>
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("ApproveIndex","HourlyMissions")">
                                        <span>تایید ماموریت ساعتی</span>
                                    </a>
                                </div>
                            </div>

                            <div class="menu-item">
                                <a style="color:white" class="ras-sidebar-ul" data-toggle="collapse" data-parent="#accordion" href="#collapse5">
                                    <i title=" تنظیمات" class="ti-settings pull-left ras-sidebar-left-logo" style="font-size:24px ; margin-left:-15px;color:white"></i>
                                    <i class="ti-settings"></i>
                                    تنظیمات
                                </a>
                                <div id="collapse5" class="panel-collapse collapse">
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("Calendar", "Settings") ">
                                        <span>تقویم سازمانی</span>
                                    </a>
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("Departments","Settings")">
                                        <span>واحد های سازمانی</span>
                                    </a>
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("Sync","Home")">
                                        <span>همگام سازی PWA</span>
                                    </a>
                                </div>
                            </div>

                            <div class="menu-item">
                                <a style="color:white" class="ras-sidebar-ul" data-toggle="collapse" data-parent="#accordion" href="#collapse6">
                                    <i title=" گزارشات" class="ti-bar-chart-alt pull-left ras-sidebar-left-logo" style="font-size:24px ; margin-left:-15px;color:white"></i>
                                    <i class="ti-bar-chart-alt"></i>
                                    گزارشات
                                </a>
                                <div id="collapse6" class="panel-collapse collapse">
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("BreakingProjectsByMonth","Reports")">
                                        <span>گزارش پروژه ها به تفکیک ماه</span>
                                    </a>
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("BreakingProjectByMonth","Reports")">
                                        <span>گزارش یک پروژه به تفکیک ماه</span>
                                    </a>
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("PersonelsAndProjects", "Reports")">
                                        <span>گزارش ماتریسی پرسنل و پروژه ها</span>
                                    </a>
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("TotalWorkHoursOnProjects", "Reports")">
                                        <span>گزارش کلی پروژه ها</span>
                                    </a>
                                    <a style="color:white" class="ras-sidebar-li" href="@Url.Action("DailyOnProjects", "Reports")">
                                        <span>گزارش روزانه پروژه ها</span>
                                    </a>
                                </div>
                            </div>
                        </div>



                    </nav>
                </div>
            </div>
        </div>



        <div class="content-body" style="overflow:auto!important">
            <script src="~/Scripts/jquery-1.10.2.js"></script>
            <script src="~/Scripts/jquery-1.10.2.min.js"></script>
            <script src="~/Scripts/Layout/bootstrap.min.js"></script>
            <script src="~/Scripts/Layout/popper.min.js"></script>
            <script src="~/Content/bootstrap.notify.js"></script>
            <script src="~/Content/bootstrap.notify.min.js"></script>
            <script src="~/Scripts/RasSideBar.js"></script>
            <script src="~/Scripts/Site.js"></script>

            @RenderBody()
        </div>
    </div>

    @RenderSection("scripts", required: false)


</body>







</html>
